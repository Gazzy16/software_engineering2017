<h1>Listing programs</h1>

<table>
  <tr>
    <th>Thread</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @programs.each do |program| %>
  <tr>
    <td><%= program.thread_id %></td>
    <td><%= link_to 'Show', [program.threadr, program] %></td>
    <td><%= link_to 'Edit', edit_threadr_program_path(program.threadr, program) %></td>
    <td><%= link_to 'Destroy', [program.threadr, program], :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Program', new_threadr_program_path(params[:threadr_id]) %>
