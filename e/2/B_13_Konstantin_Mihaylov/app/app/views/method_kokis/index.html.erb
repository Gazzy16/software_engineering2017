<p id="notice"><%= notice %></p>

<h1>Method Kokis</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Title</th>
      <th>Content</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @method_kokis.each do |method_koki| %>
      <tr>
        <td><%= method_koki.name %></td>
        <td><%= method_koki.title %></td>
        <td><%= method_koki.content %></td>
        <td><%= link_to 'Show', method_koki %></td>
        <td><%= link_to 'Edit', edit_method_koki_path(method_koki) %></td>
        <td><%= link_to 'Destroy', method_koki, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Method Koki', new_method_koki_path %>
