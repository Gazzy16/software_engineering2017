<p id="notice"><%= notice %></p>

<h1>Car Manufactures</h1>

<table>
  <thead>
    <tr>
      <th>Car</th>
      <th>Manufacture</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @car_manufactures.each do |car_manufacture| %>
      <tr>
        <td><%= car_manufacture.car_id %></td>
        <td><%= car_manufacture.manufacture_id %></td>
        <td><%= link_to 'Show', car_manufacture %></td>
        <td><%= link_to 'Edit', edit_car_manufacture_path(car_manufacture) %></td>
        <td><%= link_to 'Destroy', car_manufacture, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Car Manufacture', new_car_manufacture_path %>
