<p id="notice"><%= notice %></p>

<h1>Teachers Schools</h1>

<table>
  <thead>
    <tr>
      <th>Id teacher</th>
      <th>Id school</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @teachers_schools.each do |teachers_school| %>
      <tr>
        <td><%= teachers_school.id_teacher %></td>
        <td><%= teachers_school.id_school %></td>
        <td><%= link_to 'Show', teachers_school %></td>
        <td><%= link_to 'Edit', edit_teachers_school_path(teachers_school) %></td>
        <td><%= link_to 'Destroy', teachers_school, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Teachers School', new_teachers_school_path %>
