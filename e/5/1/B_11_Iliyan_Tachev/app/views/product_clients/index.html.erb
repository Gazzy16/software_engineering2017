<p id="notice"><%= notice %></p>

<h1>Product Clients</h1>

<table>
  <thead>
    <tr>
      <th>Product name</th>
      <th>Client name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @product_clients.each do |product_client| %>
      <tr>
        <td><%= product_client.product_name %></td>
        <td><%= product_client.client_name %></td>
        <td><%= link_to 'Show', product_client %></td>
        <td><%= link_to 'Edit', edit_product_client_path(product_client) %></td>
        <td><%= link_to 'Destroy', product_client, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Product Client', new_product_client_path %>
