<p id="notice"><%= notice %></p>

<h1>Client Product Connections</h1>

<table>
  <thead>
    <tr>
      <th>Client</th>
      <th>Product</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @client_product_connections.each do |client_product_connection| %>
      <tr>
        <td><%= client_product_connection.client_id %></td>
        <td><%= client_product_connection.product_id %></td>
        <td><%= link_to 'Show', client_product_connection %></td>
        <td><%= link_to 'Edit', edit_client_product_connection_path(client_product_connection) %></td>
        <td><%= link_to 'Destroy', client_product_connection, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Client Product Connection', new_client_product_connection_path %>
