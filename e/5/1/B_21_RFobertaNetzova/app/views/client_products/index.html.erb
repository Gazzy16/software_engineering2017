<p id="notice"><%= notice %></p>

<h1>Client Products</h1>

<table>
  <thead>
    <tr>
      <th>Client name</th>
      <th>Product name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @client_products.each do |client_product| %>
      <tr>
        <td><%= client_product.client.name %></td>
        <td><%= client_product.product.name %></td>
       
      </tr>
    <% end %>
  </tbody>
</table>

<br>


