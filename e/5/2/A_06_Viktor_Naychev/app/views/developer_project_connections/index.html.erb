<p id="notice"><%= notice %></p>

<h1>Developer Project Connections</h1>

<table>
  <thead>
    <tr>
      <th>Delevoper</th>
      <th>Project</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @developer_project_connections.each do |developer_project_connection| %>
      <tr>
        <td><%= developer_project_connection.delevoper_id %></td>
        <td><%= developer_project_connection.project_id %></td>
        <td><%= link_to 'Show', developer_project_connection %></td>
        <td><%= link_to 'Edit', edit_developer_project_connection_path(developer_project_connection) %></td>
        <td><%= link_to 'Destroy', developer_project_connection, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

