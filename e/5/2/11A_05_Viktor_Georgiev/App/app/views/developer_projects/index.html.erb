<p id="notice"><%= notice %></p>

<h1>Developer Projects</h1>

<table>
  <thead>
    <tr>
      <th>Project</th>
      <th>Developer</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @developer_projects.each do |developer_project| %>
      <tr>
        <td><%= developer_project.project_id %></td>
        <td><%= developer_project.developer_id %></td>
        <td><%= link_to 'Show', developer_project %></td>
        <td><%= link_to 'Edit', edit_developer_project_path(developer_project) %></td>
        <td><%= link_to 'Destroy', developer_project, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Developer Project', new_developer_project_path %>
