<%= form_with scope: :bottle, local: true do |form| %>
  <p>
    <%= form.label :color %><br>
    <%= form.text_field :color %>
  </p>
 
  <p>
    <%= form.label :brand %><br>
    <%= form.text_field :brand %>
  </p>
 
  <p>
    <%= form.label :table_id %><br>
    <%= form.text_field :table_id %>
  </p>

  <p>
    <%= form.submit %>
  </p>
<% end %>
