<p id="notice"><%= notice %></p>

<h1>Members Teams</h1>

<table>
  <thead>
    <tr>
      <th>Member</th>
      <th>Team</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @members_teams.each do |members_team| %>
      <tr>
        <td><%= members_team.member_id %></td>
        <td><%= members_team.team_id %></td>
        <td><%= link_to 'Show', members_team %></td>
        <td><%= link_to 'Edit', edit_members_team_path(members_team) %></td>
        <td><%= link_to 'Destroy', members_team, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Members Team', new_members_team_path %>
