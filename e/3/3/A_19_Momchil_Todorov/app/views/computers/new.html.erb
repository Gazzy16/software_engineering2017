<%= form_with scope: :computer, local: true do |form| %>
  <p>
    <%= form.label :color %><br>
    <%= form.text_field :color %>
  </p>
 
  <p>
    <%= form.label :brand %><br>
    <%= form.text_field :brand %>
  </p>

	<p>
    <%= form.label :processor %><br>
    <%= form.text_field :processor %>
  </p>

  <p>
    <%= form.label :room_id %><br>
    <%= form.text_field :room_id %>
  </p>
 
  <p>
    <%= form.submit %>
  </p>
<% end %>
